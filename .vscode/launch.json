{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) run example (gfortran)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/vscode/example/${fileBasenameNoExtension}",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "externalConsole": false,
      "MIMode": "gdb",
      "logging": {
        "engineLogging": true,
        "trace": true,
        "traceResponse": true
      },
      "setupCommands": [
        {
          "description": "Enable GDB pretty-printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Use Intel syntax for assembly",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "fpm: run example (gfortran)"
    },
